{
  "name": "acquia/drupal-minimal-project",
  "description": "Minimal Acquia-compatible Drupal application based on the Drupal Recommended Project",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Acquia Engineering",
      "homepage": "https://www.acquia.com",
      "role": "Maintainer"
    }
  ],
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  },
  "require": {
    "acquia/blt": "12.7",
    "acquia/blt-behat": "^1.1",
    "composer/installers": "^1.9",
    "cweagans/composer-patches": "^1.6",
    "drupal/admin_toolbar": "^3.0",
    "drupal/config_split": "^1.7",
    "drupal/core-composer-scaffold": "^9",
    "drupal/core-recommended": "9.1.9",
    "drupal/csv_importer": "^1.11",
    "drupal/pathauto": "^1.8",
    "drupal/rabbit_hole": "^1.0@beta",
    "drupal/s3fs": "^3.0@alpha",
    "drupal/scheduler": "^1.3",
    "drupal/scheduler_content_moderation_integration": "^1.3",
    "drupal/shield": "^1.4",
    "drupal/smtp": "^1.0",
    "drupal/views_data_export": "^1.0",
    "drush/drush": "^10.3"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "docroot/"
      },
      "file-mapping": {
        "[web-root]/sites/default/default.services.yml": {
          "mode": "replace",
          "path": "docroot/core/assets/scaffold/files/default.services.yml",
          "overwrite": false
        },
        "[web-root]/sites/default/default.settings.php": {
          "mode": "replace",
          "path": "docroot/core/assets/scaffold/files/default.settings.php",
          "overwrite": false
        }
      },
      "gitignore": true
    },
    "installer-paths": {
      "docroot/core": [
        "type:drupal-core"
      ],
      "docroot/libraries/{$name}": [
        "type:drupal-library"
      ],
      "docroot/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "docroot/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "docroot/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/Commands/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "docroot/modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "docroot/themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ]
    },
    "enable-patching": true,
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "patches": {
        "drupal/core": {
            "2719797 - New option for Views page displays to use the admin theme - feature/STRY0020001": "https://www.drupal.org/files/issues/2020-10-07/2719797-109.patch"
        },
        "drupal/scheduler_content_moderation_integration": {
            "3077147 - Scheduler content moderation integration error fix - feature/STRY0020225": "https://www.drupal.org/files/issues/2020-06-30/3077147-28.patch"
        }
    }
  },
  "autoload": {
    "psr-4": {
      "Washco\\": "blt/src/"
    }
  },
  "require-dev": {
    "acquia/blt-phpcs": "^1.0",
    "phpunit/phpunit": "^4.8.35 || ^6.5 || ^7"
  }
}
